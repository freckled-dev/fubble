[settings]
os=iOS
os.version=12.0
arch=armv8
compiler=apple-clang
compiler.version=12.0
compiler.libcxx=libc++
build_type=Debug

[options]
boost:extra_b2_flags=visibility=global
fubble:meson_cross_file=scripts/meson_cross/ios
[build_requires]
# https://docs.conan.io/en/latest/integrations/cross_platform/ios.html
#darwin-toolchain/1.0.8@theodelrieu/stable
[env]
# webrtc and boost set `visibilty` by default to hidden
CXXFLAGS=-fembed-bitcode -fvisibility=hidden -fexceptions
CFLAGS=-fembed-bitcode
LDLAGS=-fembed-bitcode

