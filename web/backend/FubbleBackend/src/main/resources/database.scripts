create table ROOM
(
   ID					VARCHAR(255)		NOT NULL PRIMARY KEY,
   NAME					VARCHAR(255)		NOT NULL,
   CONNECTEDCOUNTER		INT 
);

create table ROOM_USER
(
   ROOM_ID				VARCHAR(255)		NOT NULL,
   USER_ID				VARCHAR(255)		NOT NULL
);


create table FUBBLEUSER
(
   ID					VARCHAR(255)		NOT NULL PRIMARY KEY,
   NAME					VARCHAR(255)		NOT NULL
);